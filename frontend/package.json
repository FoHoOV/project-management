{
	"name": "todos",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build-integration": "vite build --mode integration",
		"preview": "vite preview --port 5174",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"open-api:generate:linux": "rm -rf ./src/lib/generated-client && env-cmd pnpm run open-api:generate:shared && env-cmd pnpm run open-api:zod:linux",
		"open-api:generate:windows": "(rmdir /s /q .\\src\\lib\\generated-client || dir) && env-cmd pnpm run open-api:generate:shared && env-cmd pnpm run open-api:zod:windows",
		"open-api:generate:shared": "node generate-openapitools-json.js && openapi-generator-cli generate --generator-key typescript-fetch",
		"open-api:zod:linux": "mkdir -p ./src/lib/generated-client/zod && typed-openapi $PUBLIC_API_URL/openapi.json -o ./src/lib/generated-client/zod/schemas.ts --runtime zod && pnpm run open-api:zod:shared",
		"open-api:zod:windows": "mkdir .\\src\\lib\\generated-client\\zod && typed-openapi %PUBLIC_API_URL%/openapi.json -o ./src/lib/generated-client/zod/schemas.ts --runtime zod && pnpm run open-api:zod:shared",
		"open-api:zod:shared": "node extract-zod.js"
	},
	"devDependencies": {
		"@openapitools/openapi-generator-cli": "^2.13.13",
		"@playwright/test": "^1.47.2",
		"@sveltejs/adapter-auto": "^3.2.5",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/adapter-vercel": "^5.4.4",
		"@sveltejs/kit": "^2.6.1",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
		"@tailwindcss/typography": "^0.5.15",
		"@testing-library/jest-dom": "^6.5.0",
		"@testing-library/svelte": "^5.2.3",
		"@typescript-eslint/eslint-plugin": "^8.8.0",
		"@typescript-eslint/parser": "^8.8.0",
		"@vitest/ui": "^1.6.0",
		"autoprefixer": "^10.4.20",
		"dotenv": "^16.4.5",
		"env-cmd": "^10.1.0",
		"eslint": "^9.12.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.44.1",
		"jsdom": "^24.1.3",
		"openapi-typescript-codegen": "^0.27.0",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"svelte": "5.0.0-next.262",
		"svelte-check": "^4.0.4",
		"svelte-fa": "^4.0.3",
		"tailwindcss": "^3.4.13",
		"ts-to-zod": "^3.13.0",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"vite": "^5.4.8",
		"vitest": "^1.6.0",
		"vitest-dom": "^0.1.1",
		"@types/eslint": "^9.6.1",
		"@types/js-cookie": "^3.0.6"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/free-brands-svg-icons": "^6.6.0",
		"@fortawesome/free-regular-svg-icons": "^6.6.0",
		"@fortawesome/free-solid-svg-icons": "^6.6.0",
		"ajv": "^8.17.1",
		"daisyui": "^4.12.12",
		"globals": "^15.10.0",
		"jose": "^5.9.3",
		"js-cookie": "^3.0.5",
		"typed-openapi": "^0.4.1",
		"typescript-eslint": "^8.8.0",
		"zod": "^3.23.8"
	},
	"packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}

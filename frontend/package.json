{
	"name": "tut2",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"open-api:generate": "rm -rf ./src/lib/client; openapi-generator-cli generate --generator-key typescript-fetch && npm run open-api:zod",
		"open-api:zod": "mkdirp ./src/lib/client/zod && typed-openapi http://127.0.0.1:8080/openapi.json -o ./src/lib/client/zod/schemas.ts --runtime zod && node extract-zod.js"
	},
	"devDependencies": {
		"@openapitools/openapi-generator-cli": "^2.7.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/typography": "^0.5.9",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"openapi-typescript-codegen": "^0.25.0",
		"postcss": "^8.4.27",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-fa": "^3.0.4",
		"sveltekit-search-params": "^1.0.14",
		"tailwindcss": "^3.3.3",
		"ts-to-zod": "^3.1.3",
		"tslib": "^2.4.1",
		"typescript": "^5.2.2",
		"vite": "^4.4.2",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"dependencies": {
		"@felte/validator-zod": "^1.0.16",
		"@fortawesome/free-brands-svg-icons": "^6.4.2",
		"@fortawesome/free-regular-svg-icons": "^6.4.2",
		"@fortawesome/free-solid-svg-icons": "^6.4.2",
		"@macfja/svelte-persistent-store": "^2.3.2",
		"@types/js-cookie": "^3.0.3",
		"daisyui": "^3.5.1",
		"jose": "^4.14.4",
		"js-cookie": "^3.0.5",
		"theme-change": "^2.5.0",
		"typed-openapi": "^0.1.4",
		"zod": "^3.21.4"
	}
}

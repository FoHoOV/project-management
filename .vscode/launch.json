  "version": "0.2.0",
  "configurations": [
    {
      "name": "back: FastAPI",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["main:app", "--reload", "--port", "8080"],
      "jinja": true,
      "justMyCode": true
    },
    {
      "command": "npm run dev",
      "name": "front:dev",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "npm run open-api:generate",
      "name": "front:open-api:generate",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "npm run open-api:zod",
      "name": "front:open-api:zod",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "npm run check:watch",
      "name": "front:check:watch",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "front:chrome:dev",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
